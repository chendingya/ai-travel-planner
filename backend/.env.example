PORT=3001

# AI API 配置 (支持 OpenAI 兼容格式)
# 可以使用 GitCode Kimi、阿里百炼等任何兼容 OpenAI API 的服务
AI_API_KEY=your_ai_api_key_here
AI_BASE_URL=https://api.gitcode.com/api/v5
AI_MODEL=Kimi-K2
# AI_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
# AI_MODEL=qwen3-max-preview

# 兼容旧配置：如果使用阿里百炼，也可以使用以下配置
# DASHSCOPE_API_KEY=your_dashscope_api_key_here
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
DASHSCOPE_AI_MODEL=qwen3-max-preview

# Supabase 配置（仅服务端使用 Service Role Key，不暴露给前端）
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# 腾讯云混元生图配置
TENCENT_SECRET_ID=your_tencent_secret_id_here
TENCENT_SECRET_KEY=your_tencent_secret_key_here

# Frontend runtime configuration (safe to expose to browser)
PUBLIC_SUPABASE_URL=your_supabase_url_here
PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
PUBLIC_AMAP_KEY=your_amap_web_key_here
PUBLIC_AMAP_SECURITY_CODE=your_amap_security_code_here
PUBLIC_AMAP_REST_KEY=