# ============================================
# 服务器配置
# ============================================
PORT=3001
HOST=0.0.0.0
NODE_ENV=development

# ============================================
# Supabase 配置
# ============================================
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_service_key
SUPABASE_ANON_KEY=your_supabase_anon_key

# ============================================
# 文本生成提供商配置 (按优先级顺序)
# ============================================

# 1. 魔搭社区 (ModelScope) - 最高优先级
MODELSCOPE_TEXT_ENABLED=true
MODELSCOPE_TEXT_API_KEY=your_modelscope_api_key
MODELSCOPE_TEXT_BASE_URL=https://api-inference.modelscope.cn/v1
MODELSCOPE_TEXT_MODEL=deepseek-ai/DeepSeek-V3.2

# 2. GitCode - 第二优先级
GITCODE_TEXT_ENABLED=true
GITCODE_TEXT_API_KEY=your_gitcode_api_key
GITCODE_TEXT_BASE_URL=https://api.gitcode.com/v1
GITCODE_TEXT_MODEL=deepseek-ai/DeepSeek-V3.2

# 3. 阿里百炼 (DashScope) - 第三优先级 (备用)
DASHSCOPE_ENABLED=true
DASHSCOPE_API_KEY=your_dashscope_api_key
DASHSCOPE_MODEL=qwen3-max-preview

# ============================================
# 图片生成提供商配置 (按优先级顺序)
# ============================================

# 1. 魔搭社区 (ModelScope) - 最高优先级
MODELSCOPE_IMAGE_ENABLED=true
MODELSCOPE_IMAGE_API_KEY=your_modelscope_api_key
MODELSCOPE_IMAGE_BASE_URL=https://api-inference.modelscope.cn/v1
MODELSCOPE_IMAGE_MODEL=Tongyi-MAI/Z-Image-Turbo

# 2. 腾讯混元 - 第二优先级
TENCENT_IMAGE_ENABLED=true
TENCENT_IMAGE_API_KEY=your_tencent_secret_id
TENCENT_IMAGE_SECRET_KEY=your_tencent_secret_key
TENCENT_IMAGE_REGION=ap-guangzhou

# ============================================
# 高德地图配置（前端需要）
# ============================================
AMAP_KEY=your_amap_key
AMAP_SECURITY_CODE=your_amap_security_code
AMAP_REST_KEY=your_amap_rest_key

# ============================================
# CORS 配置
# ============================================
CORS_ORIGIN=*
